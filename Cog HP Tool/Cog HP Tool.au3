#region ;Functions 1
Func Pass() ;Lowers Lure by 1
	If $LuredTurns > 0 Then
		$LuredTurns -= 1
		GUICtrlSetData($GagLabel, "Gag: You skipped a turn.")
	EndIf
	Update()
EndFunc   ;==>Pass

Func Damage() ;Damage Formula
	If $TrapDamageAmount > 0 And $LuredTurns > 0 And $HP1 > $TrapDamageAmount Then
		$HP1 -= $TrapDamageAmount
		$LuredTurns = 0
		GUICtrlSetData($CogDamageLabel, "Damage: You have dealt " & $TrapDamageAmount & " damage to the cog.")
		$TrapDamageAmount = 0
		GUICtrlSetData($TrapLabel, "Trap:")
	ElseIf $DamageAmount > 0 And $LuredTurns > 0 And $HP1 > $DamageAmount * $LureBonus Then
		$HP1 -= $DamageAmount * $LureBonus
		$LuredTurns = 0
		GUICtrlSetData($CogDamageLabel, "Damage: You have dealt " & $DamageAmount * $LureBonus & " damage to the cog.")
		$DamageAmount = 0
	ElseIf $HP1 > $DamageAmount And $LuredTurns = 0 And $DamageAmount > 0 Then
		$HP1 -= $DamageAmount
		GUICtrlSetData($CogDamageLabel, "Damage: You have dealt " & $DamageAmount & " damage to the cog.")
		$DamageAmount = 0
	ElseIf $HP1 <= $DamageAmount > 0 Or ($TrapDamageAmount > 0 And $LuredTurns > 0) Then
		ClearStatus()
	EndIf
EndFunc   ;==>Damage

Func Update() ;Updates GUI
	Damage()
	If $LuredTurns > 0 And BitAND(GUICtrlGetState($Pass), $GUI_HIDE) = $GUI_HIDE Then ;Shows LureType Dropdown
		GUICtrlSetState($Pass, $GUI_SHOW)
	ElseIf $LuredTurns = 0 And BitAND(GUICtrlGetState($Pass), $GUI_SHOW) = $GUI_SHOW Then ;Hides LureType Dropdown
		GUICtrlSetState($Pass, $GUI_HIDE)
	EndIf
	GUICtrlSetData($HPLabel, "HP: " & $HP1 & "/" & $HP2)
	$HPPercent = $HP1 / $HP2 * 100
	GUICtrlSetData($HPBar, $HPPercent)
	If $LuredTurns > 0 Then
		GUICtrlSetData($CogLureLabel, "Lure: " & $LuredTurns & " Turns Remaining.")
	ElseIf $LuredTurns = 0 Then
		GUICtrlSetData($CogLureLabel, "Lure: ")
	EndIf
	Sleep(100)
	If $LuredTurns > 0 Then
		GUICtrlSetState($Pass, $GUI_ENABLE)
	ElseIf $LuredTurns = 0 Then
		GUICtrlSetState($Pass, $GUI_DISABLE)
	EndIf
	If $HP1 / $HP2 * 100 > 95 And $HP2 > 0 Then
		GUICtrlSetState($HPGreen, $GUI_SHOW)
		GUICtrlSetState($HPYellow, $GUI_HIDE)
		GUICtrlSetState($HPOrange, $GUI_HIDE)
		GUICtrlSetState($HPRed, $GUI_HIDE)
		GUICtrlSetState($HPNone, $GUI_HIDE)
	ElseIf $HP1 / $HP2 * 100 >= 65 And $HP2 > 0 Then
		GUICtrlSetState($HPGreen, $GUI_HIDE)
		GUICtrlSetState($HPYellow, $GUI_SHOW)
		GUICtrlSetState($HPOrange, $GUI_HIDE)
		GUICtrlSetState($HPRed, $GUI_HIDE)
		GUICtrlSetState($HPNone, $GUI_HIDE)
	ElseIf $HP1 / $HP2 * 100 >= 35 And $HP2 > 0 Then
		GUICtrlSetState($HPGreen, $GUI_HIDE)
		GUICtrlSetState($HPYellow, $GUI_HIDE)
		GUICtrlSetState($HPOrange, $GUI_SHOW)
		GUICtrlSetState($HPRed, $GUI_HIDE)
		GUICtrlSetState($HPNone, $GUI_HIDE)
	ElseIf $HP1 / $HP2 * 100 > 0 And $HP2 > 0 Then
		GUICtrlSetState($HPGreen, $GUI_HIDE)
		GUICtrlSetState($HPYellow, $GUI_HIDE)
		GUICtrlSetState($HPOrange, $GUI_HIDE)
		GUICtrlSetState($HPRed, $GUI_SHOW)
		GUICtrlSetState($HPNone, $GUI_HIDE)
	Else
		GUICtrlSetState($HPGreen, $GUI_HIDE)
		GUICtrlSetState($HPYellow, $GUI_HIDE)
		GUICtrlSetState($HPOrange, $GUI_HIDE)
		GUICtrlSetState($HPRed, $GUI_HIDE)
		GUICtrlSetState($HPNone, $GUI_SHOW)
		If $HP2 > 0 Then
			GUICtrlSetData($CogDamageLabel, "Damage: Congratulations! You have defeated the cog.")
		EndIf
		ClearStatus()
		GUICtrlSetData($CogLevel, "Select")
	EndIf
EndFunc   ;==>Update

Func ClearStatus() ;Clears Variables
	$HP1 = 0
	$LuredTurns = 0
	$TrapDamageAmount = 0
	$DamageAmount = 0
	GUICtrlSetState($CogLevel, $GUI_ENABLE)
EndFunc   ;==>ClearStatus

Func Close() ;Closes Gag GUI and Enables Main GUI
	If $HP1 > 0 Then
		If BitAND(GUICtrlGetState($GagsButton), $GUI_HIDE) = $GUI_HIDE And WinGetState("Gags") < 7 Then
			GUICtrlSetState($GagsButton, $GUI_SHOW)
		EndIf
		If BitAND(GUICtrlGetState($CogsButton), $GUI_HIDE) = $GUI_HIDE And WinGetState("Cogs") < 7 Then
			GUICtrlSetState($CogsButton, $GUI_SHOW)
		EndIf
	ElseIf $HP1 = 0 Then
		If BitAND(GUICtrlGetState($GagsButton), $GUI_SHOW) = $GUI_SHOW Then
			GUICtrlSetState($GagsButton, $GUI_HIDE)
		EndIf
		If BitAND(GUICtrlGetState($CogsButton), $GUI_SHOW) = $GUI_SHOW Then
			GUICtrlSetState($CogsButton, $GUI_HIDE)
		EndIf
	EndIf
	If BitAND(GUICtrlGetState($CogLevel), $GUI_DISABLE) = $GUI_DISABLE And WinGetState("Gags") < 7 And WinGetState("Cogs") < 7 Then
		GUICtrlSetState($CogLevel, $GUI_ENABLE)
	EndIf
EndFunc   ;==>Close

