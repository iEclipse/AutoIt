		$CogDept = "Bossbot"
	ElseIf BitAND(GUICtrlGetState($Cashbots), $GUI_SHOW) = $GUI_SHOW Then
		$CogDept = "Cashbot"
	ElseIf BitAND(GUICtrlGetState($Lawbots), $GUI_SHOW) = $GUI_SHOW Then
		$CogDept = "Lawbot"
	ElseIf BitAND(GUICtrlGetState($Sellbots), $GUI_SHOW) = $GUI_SHOW Then
		$CogDept = "Sellbot"
	EndIf
	Global $CogAbilities = Eval(StringReplace(StringReplace(StringStripWS($CogName, $STR_STRIPALL), "-", ""), ".", "") & GUICtrlRead($CogLevel))
	GUICtrlSetState($IDCard, $GUI_SHOW)
	GUICtrlSetState($ID, $GUI_SHOW)
	GUICtrlSetData($CogNameLabel, "Name: " & $CogName)
	GUICtrlSetData($CogDeptLabel, "Dept: " & $CogDept)
	GUICtrlSetData($CogWeaknessLabel, "Weakness: " & $CogWeakness)
	GUICtrlSetData($CogAbilitiesTitleLabel, "Abilities: ")
	GUICtrlSetData($CogAbilitiesLabel, $CogAbilities)
	CogReset()
	GUISetState(@SW_HIDE, $Cogs)
	Close()
EndFunc   ;==>CogName
